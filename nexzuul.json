{
    "id": "nexgate/nexzuul",
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "container": {
      "portMappings": [
        {
          "containerPort": 9999,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 10100
        }
      ],
      "type": "DOCKER",
      "volumes": [],
      "docker": {
        "image": "nexclipper/nexzuul",
        "forcePullImage": true,
        "privileged": false,
        "parameters": []
      }
    },
    "cpus": 1,
    "disk": 0,
    "healthChecks": [
      {
        "gracePeriodSeconds": 300,
        "intervalSeconds": 60,
        "maxConsecutiveFailures": 3,
        "portIndex": 0,
        "timeoutSeconds": 20,
        "delaySeconds": 15,
        "protocol": "MESOS_HTTP",
        "path": "/check"
      }
    ],
    "instances": 1,
    "maxLaunchDelaySeconds": 3600,
    "mem": 1024,
    "gpus": 0,
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "requirePorts": false,
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 0,
      "expungeAfterSeconds": 0
    },
    "fetch": [],
    "constraints": []
  }